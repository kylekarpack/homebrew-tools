{
	"name": "homebrew-tools",
	"version": "0.2.0",
	"private": false,
	"license": "MIT",
	"dependencies": {
		"@ant-design/icons": "^4.6.4",
		"antd": "^4.22.8",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-router-dom": "^5.3.4",
		"react-scripts": "^5.0.1",
		"recoil": "^0.7.6"
	},
	"devDependencies": {
		"@types/jest": "^25.2.3",
		"@types/node": "^14.0.5",
		"babel-preset-react-app": "^9.1.2",
		"cross-env": "^7.0.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"jest-sonar-reporter": "^2.0.0",
		"react-test-renderer": "^16.13.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "cross-env CI=true react-scripts test",
		"eject": "react-scripts eject"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"node_modules",
			"serviceWorker.js",
			"index.js"
		],
		"coverageReporters": [
			"lcov",
			"text"
		]
	},
	"jestSonar": {
		"reportPath": "reports",
		"reportFile": "test-report.xml",
		"indent": 4
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
